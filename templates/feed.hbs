<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom" xml:lang="en">
    <generator uri="https://theseus-os.github.io/blog/{{blog.prefix}}" version="0.1.0">{{blog.title}}</generator>
    <link href="https://theseus-os.github.io/blog/{{blog.prefix}}feed.xml" rel="self" type="application/atom+xml" />
    <link href="https://theseus-os.github.io/blog/{{blog.prefix}}" rel="alternate" type="text/html" />
    <id>https://theseus-os.github.io/blog/{{blog.prefix}}</id>
    <title>{{blog.title}}</title>
    <subtitle>{{blog.description}}</subtitle>
    <author>
        <name>Maintained by {{blog.maintained_by}}.</name>
        <uri>https://github.com/theseus-os/blog/</uri>
    </author>
    <updated>{{feed_updated}}</updated>

    {{#each posts}}
    <entry>
        <title>{{title}}</title>
        <link rel="alternate" href="https://theseus-os.github.io/blog/{{../blog.prefix}}{{url}}" type="text/html" title="{{title}}" />
        <published>{{published}}</published>
        <updated>{{updated}}</updated>
        <id>https://theseus-os.github.io/blog/{{../blog.prefix}}{{url}}</id>
        <content type="html" xml:base="https://theseus-os.github.io/blog/{{../blog.prefix}}{{url}}">{{contents}}</content>

        <author>
            <name>{{author}}</name>
        </author>
    </entry>
    {{/each}}
</feed>
